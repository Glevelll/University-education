<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="256.0" prefWidth="240.0" style="-fx-background-color: #720f0f;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.genealogicaltree.RegistrationController">
    <children>
        <Label layoutX="50.0" layoutY="50.0" text="Введите логин и пароль" textFill="#ffc9c9" />
        <Label layoutX="30.0" layoutY="90.0" text="Логин" textFill="WHITE" />
        <Label layoutX="25.0" layoutY="130.0" text="Пароль" textFill="WHITE" />
        <TextField fx:id="log" layoutX="70.0" layoutY="85.0" />
        <TextField fx:id="pass" layoutX="70.0" layoutY="125.0" />
        <Button fx:id="save" layoutX="80.0" layoutY="170.0" mnemonicParsing="false" onAction="#saveInfo" text="Сохранить" />
        <Button fx:id="back" layoutX="90.0" layoutY="200.0" mnemonicParsing="false" onAction="#backToMain" text="Назад" />
    </children>
</AnchorPane>
